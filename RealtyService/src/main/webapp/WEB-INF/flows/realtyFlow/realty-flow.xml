<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
	start-state="realty-entry-point">	

	<view-state id="realty-entry-point" view="home.xhtml">	
		<on-entry>
			<evaluate expression="realtyController.getWizard()" result="viewScope.realtyWizard"></evaluate>
		</on-entry>
	</view-state>

	<end-state id="end" view="" />

	<global-transitions>
		<transition on="exit" to="end" />
	</global-transitions>

</flow>